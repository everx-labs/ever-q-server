{
    "name": "ton-q-server",
    "version": "0.66.0",
    "description": "TON Q Server â€“ realtime queries over TON blockchain.",
    "main": "index.js",
    "repository": "git@github.com:tonlabs/ton-q-server.git",
    "scripts": {
        "format": "npm run eslint-fix",
        "gen": "npm run gen-schema-no-format && npm run gen-blockchain-no-format && npm run format",
        "gen-schema": "npm run gen-schema-no-format && npm run format",
        "gen-blockchain": "npm run gen-blockchain-no-format && npm run format",
        "options-docs": "node dist/maintanance/doc-options/index.js",
        "tsc": "tsc --build --force",
        "test": "jest -i --forceExit",
        "start": "node dist/server/index.js",
        "update indexes": "node dist/maintanance/arango-update/index.js",
        "npm install": "npm install",
        "eslint": "eslint . --ext .ts",
        "eslint-fix": "eslint . --ext .ts --fix",
        "gen-schema-no-format": "node dist/maintanance/gen-graphql/db-schema-generator.js",
        "gen-blockchain-no-format": "graphql-codegen --config src/maintanance/blockchain-codegen.yml"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "husky": {
        "hooks": {
            "pre-commit": "npm run tsc && npm run gen && npm run tsc && git add src/server/graphql/resolvers-generated.ts res/type-defs-generated.graphql"
        }
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.347.1",
        "@eversdk/core": "^1.44.1",
        "@eversdk/lib-node": "^1.44.1",
